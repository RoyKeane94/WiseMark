{
  "$schema": "https://schema.railpack.com",
  "provider": "python",
  "steps": {
    "install": {
      "commands": ["pip install -r requirements.txt"]
    }
  },
  "deploy": {
    "inputs": [
      "...",
      {
        "step": "packages:mise",
        "include": ["/mise/shims", "/mise/installs"]
      }
    ],
    "startCommand": "bash start.sh"
  }
}
